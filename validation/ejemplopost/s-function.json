{
  "name": "ejemplopost",
  "runtime": "nodejs4.3",
  "description": "Serverless Lambda function for project: latasa",
  "customName": false,
  "customRole": false,
  "handler": "handler.handler",
  "timeout": 6,
  "memorySize": 1024,
  "authorizer": {},
  "custom": {},
  "endpoints": [
    {
      "path": "ejemplopost",
      "method": "post",
      "type": "AWS",
      "authorizationType": "none",
      "authorizerFunction": false,
      "apiKeyRequired": false,
      "requestParameters": {},
